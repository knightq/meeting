<%= meeting_table do |d|
    cell_text = "#{d.mday}<br />"
    cell_attrs = {:class => 'day'}
    @meetings.each do |e|
      if e.date == d
        cell_attrs[:class] = 'specialDay'
	cell_text = %{
	   <div class="top_left_corner">#{d.mday}</div> 
           <img src="#{e.image_url}" height="24px"/>
	}
      end
    end
    [cell_text, cell_attrs]
  end
%>

